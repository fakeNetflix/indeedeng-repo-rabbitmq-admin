{
  "type": "object",
  "description": "Describes a client connection to the RabbitMQ service.",
  "properties": {
    "name": {
      "type": "string"
    },
    "node": {
      "type": "string"
    },
    "user": {
      "type": "string"
    },
    "host": {
      "type": "string"
    },
    "port": {
      "type": "integer"
    },
    "peer_host": {
      "type": "string"
    },
    "peer_port": {
      "type": "integer"
    },
    "protocol": {
      "type": "string"
    },
    "vhost": {
      "type": "string"
    },
    "channels": {
      "type": "integer"
    },
    "channel_max": {
      "type": "integer"
    },
    "connected_at": {
      "type": "integer"
    },
    "state": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "recv_cnt": {
      "type": "integer"
    },
    "recv_oct": {
      "type": "integer"
    },
    "recv_oct_details": {
      "$ref": "Details.json"
    },
    "send_cnt": {
      "type": "integer"
    },
    "send_oct": {
      "type": "integer"
    },
    "send_oct_details": {
      "$ref": "Details.json"
    },
    "send_pend": {
      "type": "integer"
    },
    "frame_max": {
      "type": "integer"
    },
    "timeout": {
      "type": "integer"
    },
    "auth_mechanism": {
      "type": "string"
    },
    "peer_cert_issuer": {
      "type": "string"
