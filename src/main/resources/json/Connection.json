        "capabilities": {
          "type": "object",
          "properties": {
            "exchange_exchange_bindings": {
              "type": "boolean"
            },
            "consumer_cancel_notify": {
              "type": "boolean"
            },
            "publisher_confirms": {
              "type": "boolean"
            },
            "basic.nack": {
              "type": "boolean"
            }
          }
        },
        "copyright": {
          "type": "string"
        },
        "version": {
          "type": "string"
        }
      }
