      "type": "string"
    },
    "peer_cert_validity": {
      "type": "string"
    },
    "ssl": {
      "type": "boolean"
    },
    "ssl_cipher": {
      "type": "string"
    },
    "ssl_hash": {
      "type": "string"
    },
    "ssl_key_exchange": {
      "type": "string"
    },
    "ssl_protocol": {
      "type": "string"
    },
